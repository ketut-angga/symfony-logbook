{
    "name": "solvrtech/symfony-logbook",
    "description": "The Package provide integrate the client app into logbook app.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Solvrtech\\Logbook\\": "src/"
        }
    },
    "authors": [
        {
            "name": "solvrtech"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "~5.0|~6.0",
        "symfony/config": "~5.0|~6.0",
        "symfony/http-client": "~5.0|~6.0",
        "symfony/http-kernel": "~5.0|~6.0",
        "symfony/monolog-bridge": "~5.0|~6.0",
        "symfony/monolog-bundle": "^3.8",
        "monolog/monolog": "^2|^3",
        "symfony/console": "~5.0|~6.0",
        "symfony/cache": "~5.0|~6.0",
        "symfony/security-bundle": "~5.0|~6.0",
        "doctrine/doctrine-bundle": "^2.9",
        "ketut/random-string": "^1.0",
        "symfony/process": "~5.0|~6.0",
        "doctrine/orm": "^2.11"
    },
    "require-dev": {
        "symfony/yaml": "~5.0|~6.0",
        "symfony/phpunit-bridge": "~5.0|~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}